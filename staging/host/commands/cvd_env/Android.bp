package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

cc_binary_host {
    name: "cvd_internal_env",
    srcs: [
        "main.cc",
    ],
    shared_libs: [
        "libbase",
        "libcuttlefish_utils",
        "libgrpc++",
        "libjsoncpp",
        "libprotobuf-cpp-full",
    ],
    static_libs: [
        "grpc_cli_libs",
        "libabsl_host",
        "libcuttlefish_control_env",
        "libcuttlefish_host_config",
        "libgflags",
    ],
    cflags: [
        "-Wno-unused-parameter",
    ],
    cpp_std: "c++17",
    defaults: ["cuttlefish_host"],
    target: {
        darwin: {
            enabled: true,
        },
    },
}
